{
  "name": "HyperSwap",
  "description": "Customized redirection of http requests",
  "version": "1.03",
  "manifest_version": 2,
  "background": {"scripts":["background.js"]},
  "options_page": "hyperswap.html",
    "browser_action": {
		"default_icon": "hyperswap.png",
		"default_popup": "hyperswap.html" },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' https://*.googleapis.com https://*.bootstrapcdn.com; object-src 'self'"
}
